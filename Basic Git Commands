git --version
git config --global user.name "..."
git config --global user.email "..."
gig config --list
mkdir Code
git init --initial-branch=main
git status
git help

#Pro Git: https://git-scm.com/book/en/v2

git checkout -b 1690-fix1                 # make a new branh and switch to it
git checkout <branch name>                # to move to a branch like master
git add filename                          # add a file or change
git commit -a -m "this is a message about what youre committing"   # -a comment about all files changes -m comment about specefic file changes
git diff
git pull origin master                     # update all the files inside the main branch (mater)
git push origin <my branch>                # confirm changes on the branch
git log                                    # to see the list of changes; press q to exit from view
git status                                 # what branch I am on


git init
git remote add origin <git link>
git remote -v
git add .
git commit - "comment"
git push origin master
